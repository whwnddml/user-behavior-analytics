{
  "name": "user-behavior-analytics-backend",
  "version": "1.0.0",
  "description": "Backend API server for user behavior analytics",
  "main": "dist/server/index.js",
  "scripts": {
    "start:dev": "NODE_ENV=development ts-node-dev src/server/index.ts",
    "start": "NODE_ENV=production node dist/server/index.js",
    "dev": "nodemon src/server/index.ts",
    "build": "tsc",
    "build:render": "npm install && tsc && npm run db:migrate",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write src/**/*.{js,ts,json}",
    "db:migrate": "node dist/utils/migrate.js",
    "db:seed": "node dist/utils/seed.js"
  },
  "keywords": [
    "analytics",
    "user-behavior",
    "api",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.1",
    "@types/pg": "^8.15.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "typescript": "^5.3.3",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
